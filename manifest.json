{
    "name": "URL Sanitizer",
    "description": "Sanitize web page URL by striping tracking parameters",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["scripting", "tabs", "clipboardWrite"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/logo.png"
          }
      },
      "icons": {
        "128": "/images/logo.png"
      }
  }